{
	"name": "serato/sws-bootstrap",
	"homepage": "https://github.com/serato/web-sws-php-bootstrap",
	"description": "Bootstrap functionality for SWS web applications",
	"type": "library",
	"require": {
		"php" : "^7.0",
		"slim/slim": "^3.5.0",
		"serato/jwt": "^0.1.0",
		"aws/aws-sdk-php": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.0",
		"squizlabs/php_codesniffer": "~2.0",
		"symfony/cache": "dev-master#16b647274f0c6e5045284f72125316904b028034"
	},
	"autoload": {
		"psr-4": {
			"Serato\\SwsApp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\SwsApp\\Test\\": "tests/"
		}
	}
}
